    "lint": "eslint . --ext .ts,.tsx",
    "start": "electron ."
  },
  "overrides": {
    "glob": "^10.4.5"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "dompurify": "^3.2.6",
    "electron-store": "^8.2.0",
    "framer-motion": "^11.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "better-sqlite3": "^12.4.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^33.4.8",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "eslint": "^8.57.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.12",
    "wait-on": "^8.0.1"
